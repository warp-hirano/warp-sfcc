{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "packageDir": "./"
      }
    ],
    "react/react-in-jsx-scope": "off",
    "react/prop-types": 0,
    "import/no-unresolved": 0,
    "object-curly-newline": "off",
    "operator-linebreak": "off",
    "jsx-a11y/anchor-is-valid": 0,
    "import/no-named-default": "off",
    "react/no-unknown-property": "off",
    "import/no-cycle": "off",
    "no-console": "off",
    "no-alert": "off"
  },
  "globals": {
    "React": "writable"
  }
}